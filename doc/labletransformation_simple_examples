(require "../src/Labeltransformation.rkt" redex rackunit "../src/Grammar.rkt")



(println "x-out-dyn")
(judgment-holds
 (>
  (((port 1) ((7))))
  ((aPotentiallyTooSecretVariable ((4 5) (6))))
  ()
  (out ((port 1) < aPotentiallyTooSecretVariable)) : Γ C)
 C)

(println "x-out")
(judgment-holds
 (>
  (((port 1) ((1 4 5)(1 6))))
  ((aSurelySafeVariable ((4 5) (6))))
  ((1))
  (out ((port 1) < aSurelySafeVariable)) : Γ C)
 C)